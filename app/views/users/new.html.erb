<div class="header">
  <div class="container">
    <h1>New Guest Entry</h1>
  </div>
</div>

<div class="create">
  <div class="container">

<%= form_for(@new_user) do |f| %>
    <div class="field">
      <%= f.label :name %><br>
      <%= f.text_field :name %><br>
    </div>
    <div class="actions">
      <%= f.submit "Add" %>
    </div>
    <% end %>

  </div>
</div>
